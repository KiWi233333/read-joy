{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "prepare": "nuxi prepare",
    "dev": "nuxt dev --dotenv .env.development",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "generate": "nuxi generate --dotenv .env.development",
    "build": "nuxi build --dotenv .env.development",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "typecheck": "vue-tsc --noEmit",
    "release": "npm version minor && git push --tag && git fetch && git push",
    "release:major": "npm version major && git push --tag && git fetch && git push",
    "release:minor": "npm version minor && git push --tag && git fetch && git push",
    "release:patch": "npm version patch && git push --tag && git fetch && git push",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.464.0",
    "radix-vue": "^1.9.10",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul-vue": "^0.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/twemoji": "^1.2.1",
    "@nuxt/devtools": "^1.6.3",
    "@nuxt/eslint": "^0.7.2",
    "@nuxt/image": "1.8.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/tailwindcss": "6.12.2",
    "@pinia/nuxt": "^0.8.0",
    "@unocss/eslint-config": "^0.65.0",
    "@unocss/nuxt": "^0.65.0",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/nuxt": "^12.0.0",
    "consola": "^3.2.3",
    "eslint": "^9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "nuxt": "^3.14.1592",
    "pinia": "^2.2.8",
    "pinia-plugin-persistedstate": "^4.1.3",
    "sass": "^1.82.0",
    "shadcn-nuxt": "0.11.3",
    "typescript": "5.7.2",
    "vue-tsc": "^2.1.10"
  },
  "resolutions": {
    "unplugin": "2.0.0-beta.1",
    "vite": "^6.0.2",
    "vite-plugin-inspect": "^0.10.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue,js,sass,css}": [
      "eslint --fix"
    ]
  }
}

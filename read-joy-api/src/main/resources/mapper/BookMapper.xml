            <result property="categoryid" column="categoryId" jdbcType="INTEGER"/>
            <result property="author" column="author" jdbcType="VARCHAR"/>
            <result property="introduction" column="introduction" jdbcType="VARCHAR"/>
            <result property="price" column="price" jdbcType="DOUBLE"/>
            <result property="publisher" column="publisher" jdbcType="VARCHAR"/>
            <result property="coverimageurl" column="coverImageUrl" jdbcType="VARCHAR"/>
            <result property="publishiondate" column="publishionDate" jdbcType="TIMESTAMP"/>
